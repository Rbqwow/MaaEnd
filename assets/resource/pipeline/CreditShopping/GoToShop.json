{
    "CreditShoppingMain": {
        "doc": "信用点购物主入口",
        "next": [
            "[JumpBack]CancelExit",
            "InMainWindowCreditShipping",
            "[JumpBack]CloseButtonType1"
        ]
    },
    "InMainWindowCreditShipping": {
        "doc": "在主页面或者信用点购物界面",
        "recognition": {
            "type": "Or",
            "param": {
                "any_of": [
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "template": "Resell/inGame1.png",
                                "threshold": 0.8,
                                "roi": [
                                    172,
                                    0,
                                    183,
                                    119
                                ]
                            }
                        }
                    },
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "template": "Resell/inGame2.png",
                                "threshold": 0.8,
                                "roi": [
                                    1182,
                                    0,
                                    98,
                                    128
                                ]
                            }
                        }
                    },
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "template": "CreditShopping/CreditShoppingTabSelected.png",
                                "threshold": 0.7,
                                "roi": [
                                    813,
                                    44,
                                    293,
                                    82
                                ]
                            }
                        }
                    }
                ]
            }
        },
        "next": [
            "CreditShoppingShopping",
            "CreditShoppingCheckShopPage",
            "EntryCreeditShop",
            "CreditShoppingCheckMainUI"
        ]
    },
    "CreditShoppingCheckMainUI": {
        "doc": "检测主界面（未展开），点击 F5 展开",
        "post_wait_freezes": {
            "time": 200,
            "timeout": 1000
        },
        "recognition": "TemplateMatch",
        "template": "CreditShopping/Expand.png",
        "threshold": 0.8,
        "green_mask": true,
        "roi": [
            700,
            0,
            580,
            83
        ],
        "action": "ClickKey",
        "key": 192,
        "next": [
            "CreditShoppingCheckShopPage",
            "EntryCreeditShop",
            "CreditShoppingCheckMainUI"
        ]
    },
    "EntryCreeditShop": {
        "doc": "进入商店",
        "recognition": "TemplateMatch",
        "template": "CreditShopping/ShoppingEntry.png",
        "roi": [
            700,
            0,
            425,
            80
        ],
        "action": "ClickKey",
        "key": 116,
        "next": [
            "CreditShoppingCheckShopPage",
            "EntryCreeditShop"
        ]
    },
    "CreditShoppingCheckShopPage": {
        "doc": "检测商店页面，点击信用交易所 Tab",
